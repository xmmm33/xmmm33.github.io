<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Index</display-name>
  <servlet>
    <servlet-name>BPTreeIndexFileServlet</servlet-name>
    <servlet-class>my.IndexServlet.BPTreeIndexFileServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>BPTreeIndexFileServlet</servlet-name>
    <url-pattern>/fileIndex</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>ShowDiskNameServlet</servlet-name>
  	<servlet-class>my.IndexServlet.ShowDiskNameServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ShowDiskNameServlet</servlet-name>
  	<url-pattern>/diskname</url-pattern>
  </servlet-mapping>
</web-app>